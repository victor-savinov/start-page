{
    "manifest_version": 2,
    "name": "Start Page",
    "version": "1.0",
    "default_locale": "en",
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "permissions": [
        "storage",
        "<all_urls>"
    ]
}